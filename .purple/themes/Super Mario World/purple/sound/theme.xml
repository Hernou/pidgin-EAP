<?xml version="1.0" encoding="UTF-8"?>
<theme type="sound" 
	name="Super Mario World Sound Theme" 
	author="Darius Seiffert" 
	image="logo.png">

	<description>This is a soundset from the SNES game, Super Mario World. Thanks for the sounds, cortjezter</description>

	<event name="login" file="buddyon.ogg"/>
	<event name="logout" file="buddyoff.ogg"/>
	<event name="im_recv" file="receive.ogg"/>
	<event name="first_im_recv" file="new.ogg"/>
	<event name="send_im" file="send.ogg"/>
	<event name="join_chat" file="idle.ogg"/>
	<event name="left_chat" file="away.ogg"/>
	<event name="send_chat_msg" file="send.ogg"/>
	<event name="chat_msg_recv" file="receive.ogg"/>
	<event name="got_attention" file="new.ogg"/>
	<event name="nick_said" file="new.ogg"/>
</theme>
